<h1 class="non-home-page-title">Login</h1>


<%= form_tag sessions_path do %>
<div class="form-group">
  <%= label_tag :email, nil, :autofocus => true %>
  <%= text_field_tag :email %>
</div>

<div class="form-group">
  <%= label_tag :password %>
  <%= password_field_tag :password %>
</div>

<div class="form-group">
  <%= submit_tag 'Login', :class => "button-one" %>
</div>
<% end %>
